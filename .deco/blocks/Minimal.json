{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0",
    "--btn-focus-scale": "0.95",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "400"
          },
          {
            "weight": "500"
          },
          {
            "weight": "600"
          }
        ],
        "family": "Inter"
      },
      {
        "variations": [
          {
            "weight": "400"
          }
        ],
        "family": "Handlee"
      }
    ]
  },
  "mode": "light",
  "mainColors": {
    "base-100": "#ffffff",
    "primary": "#FFE68C",
    "secondary": "#9DDCFF",
    "tertiary": "#FFC9F0",
    "neutral": "#9934A6",
    "info": "#2e72d2",
    "error": "#d82c0d",
    "warning": "#ffc453",
    "success": "#7CF072"
  },
  "complementaryColors": {
    "primary-content": "#EECD56",
    "base-content": "#0C0C0C",
    "tertiary-content": "#DB4A90",
    "secondary-content": "#5AB5E8",
    "success-content": "#4ED543",
    "neutral-content": "#706F6F"
